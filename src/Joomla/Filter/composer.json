{
    "name": "joomla/filter",
    "type": "joomla-package",
    "description": "Joomla Filter Package",
    "keywords": ["joomla", "framework", "filter"],
    "homepage": "https://github.com/joomla/joomla-framework-filter",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
        "joomla/string": "1.*"
    },
    "require-dev": {
        "joomla/language": "1.*"
    },
    "suggest": {
        "joomla/language": "Required only if you want to use `OutputFilter::stringURLSafe`."
    },
    "target-dir": "Joomla/Filter",
    "autoload": {
    	"psr-0": {
    		"Joomla\\Filter": ""
    	}
    }
}
