{
    "name": "joomla/registry",
    "type": "joomla-package",
    "description": "Joomla Registry Package",
    "keywords": ["joomla", "framework", "registry"],
    "homepage": "https://github.com/joomla/joomla-framework-registry",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
         "joomla/compat": "1.0-beta2",
         "joomla/utilities": "1.0-beta2"
    },
    "require-dev": {
         "symfony/yaml": "~2.0",
         "joomla/test": "1.0-beta2"
    },
    "suggest": {
        "symfony/yaml": "Install 2.* if you require YAML support."
    },
    "target-dir": "Joomla/Registry",
    "autoload": {
    	"psr-0": {
    		"Joomla\\Registry": ""
    	}
    }
}
